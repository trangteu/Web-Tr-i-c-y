                                    <h5><%= formatCurrency(sp.Gia) %></h5>
                                </div>
                            </div>
                        </div>
                    <% }); %> 
                <% } %>                
            </div>

            <script type="text/javascript">
                $("#btnAll").click(function () {
                    $("#an-hien").show();
                    $("#an-hien-1").hide();
                    $("#an-hien-2").hide();                  
                });
        
                $("#btnTrongNuoc").click(function () {
                    $("#an-hien").hide();
                    $("#an-hien-1").show();
                    $("#an-hien-2").hide();                  
                });
        
                $("#btnNhapKhau").click(function () {
                    $("#an-hien").hide();
                    $("#an-hien-1").hide();
                    $("#an-hien-2").show();                  
                });
            </script>

