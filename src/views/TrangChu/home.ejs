            <div id="an-hien"  class="row featured__filter">
                <% if (listSP && listSP.length > 0) { %>
                    <!-- Hiển thị all sản phẩm  -->
                    <% listSP.forEach(function(sp) { %>
                        <div class="col-lg-3 col-md-4 col-sm-6 mix oranges fresh-meat">
                            <div class="featured__item">
                                <div class="featured__item__pic set-bg" data-setbg="Home/img/<%= sp.Anh %>">
                                    <ul class="featured__item__pic__hover">
                                        <li><a href="#"><i class="fa fa-heart"></i></a></li>
                                        <li><a href="#"><i class="fa fa-retweet"></i></a></li>
                                        <li><a href="#"><i class="fa fa-shopping-cart"></i></a></li>
                                    </ul>
                                </div>
                                <div class="featured__item__text">
                                    <h6><a href="/chitietsp?idChiTietSP=<%= sp.ID %>"><%= sp.Ten %></a></h6>
                                    <h5><%= formatCurrency(sp.Gia) %></h5>
                                </div>
                            </div>
                        </div>
                    <% }); %> 
                <% } %>                
            </div>

            <div id="an-hien-1"  class="row featured__filter"  style="display: none;">
