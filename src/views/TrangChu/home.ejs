                            <li id="btnAll"  class="active" data-filter="*">Tất cả</li>
                            <li id="btnTrongNuoc" data-filter=".oranges">Trái cây trong nước</li>
                            <li id="btnNhapKhau" data-filter=".fresh-meat">Trái cây nhập khẩu</li>
  
